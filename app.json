{
  "expo": {
    "name": "league",
    "slug": "leaguechampion",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app-icon.png",
    "scheme": "league",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "privacy": "public",
    "privacyPolicy": "https://yourdomain.com/privacy-policy",
    "termsOfService": "https://yourdomain.com/terms-of-service",
    "splash": {
      "image": "./assets/app-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0f172a"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dimalit778.league",
      "userInterfaceStyle": "dark",
      "buildNumber": "1",
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "backgroundColor": "#0f172a",
      "adaptiveIcon": {
        "foregroundImage": "./assets/app-icon.png",
        "backgroundColor": "#0f172a"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.dimalit778.league"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/app-icon.png"
    },
    "plugins": [
      "expo-router",

      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.834363700117-m3ta90sugtdcr7h6fb6ccot7e5rb2s9f"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos.",
          "savePhotosPermission": "Allow $(PRODUCT_NAME) to save photos.",
          "cameraPermission": "Allow $(PRODUCT_NAME) to use the camera for your avatar.",
          "isAccessMediaLocationEnabled": true,
          "granularPermissions": ["audio", "photo"]
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/app-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#0f172a"
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "infoPlist": {
              "NSCameraUsageDescription": "We use your photo library to let you upload an avatar for your league profile.",
              "NSLocalNetworkUsageDescription": "Allow local network for development",
              "NSBonjourServiceTypes": ["_expo._tcp"],
              "NSAppTransportSecurity": {
                "NSAllowsLocalNetworking": true,
                "NSExceptionDomains": {
                  "keuavfvgwhwckqordjbp.supabase.co": {
                    "NSIncludesSubdomains": true,
                    "NSExceptionRequiresForwardSecrecy": true,
                    "NSExceptionMinimumTLSVersion": "TLSv1.2",
                    "NSTemporaryExceptionAllowsInsecureHTTPLoads": false
                  },
                  "sentry.io": {
                    "NSIncludesSubdomains": true,
                    "NSExceptionRequiresForwardSecrecy": true,
                    "NSExceptionMinimumTLSVersion": "TLSv1.2",
                    "NSTemporaryExceptionAllowsInsecureHTTPLoads": false
                  },
                  "localhost": {
                    "NSIncludesSubdomains": false,
                    "NSTemporaryExceptionAllowsInsecureHTTPLoads": true,
                    "NSTemporaryExceptionMinimumTLSVersion": "1.2"
                  },
                  "127.0.0.1": {
                    "NSIncludesSubdomains": false,
                    "NSTemporaryExceptionAllowsInsecureHTTPLoads": true
                  }
                }
              }
            }
          }
        }
      ],
      "expo-secure-store",
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "leaguechampion",
          "organization": "dima-apps"
        }
      ],
      "expo-font",
      "expo-web-browser",
      [
        "expo-notifications",
        {
          "icon": "./assets/app-icon.png",
          "color": "#0f172a",
          "sounds": []
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "0730f614-1c6a-437b-b74b-508698797f3e"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/0730f614-1c6a-437b-b74b-508698797f3e"
    },
    "runtimeVersion": "1.0.0",
    "owner": "dimalit778"
  }
}
